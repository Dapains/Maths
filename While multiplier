num1 = int(input("enter an int"))
while(num1 <0):
    num1 = int(input("enter a positive int"))
num2 = int(input("enter an interger"))
while(num2 <0):
    num2 = int(input("enter a positive int"))
    
while (num1*num2=0):
    print(num1*num2)
    num1 = int(input("enter an int"))
    num2 = int(input("enter an interger"))
print(0)
print ("program finished")
