def coinconvert():
    start = input("Enter your starting currency(Either CD,US EU)")
    while start != "Quit":
        if start == "CD":
            choice = input("What currency are you converting to?")
            while choice == "CD":
                choice = input("enter a different currency from your starting one")
            if choice == "US":
                amount = int(input("Enter the amount you are converting"))
                while amount <= 0:
                    amount = int(input("Enter a positive amount"))
                feetype = input("will this be cash or check?")
                while feetype != "cash" and feetype != "check":
                    feetype = input("will this be cash or check?")
                if feetype == "cash":
                    fee = (amount * .05)
                    print("Your fee is:", fee)
                elif feetype == "check":
                    fee = (amount * .01)
                    print("Your fee is:", fee)
                total = (amount * .78)
                print ("Your total is:", total)
            if choice == "EU":
                amount = int(input("Enter the amount you are converting"))
                while amount <= 0:
                    amount = int(input("Enter a positive amount"))
                feetype = input("will this be cash or check?")
                while feetype != "cash" and feetype != "check":
                    feetype = input("will this be cash or check?")
                if feetype == "cash":
                    fee = (amount * .05)
                    print("Your fee is:", fee)
                elif feetype == "check":
                    fee = (amount * .01)
                    print("Your fee is:", fee)
                total = (amount * .78)
                print ("Your total is:", total)
        elif start == "US":
            choice = input("What currency are you converting to?")
            while choice == "US":
                choice = input("enter a different currency from your starting one")
            if choice == "CD":
